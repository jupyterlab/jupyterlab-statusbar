{
  "name": "jupyterlab-statusbar",
  "version": "0.1.0",
  "description": "A status bar for JupyterLab",
  "keywords": [
    "jupyter",
    "jupyterlab",
    "jupyterlab-extension"
  ],
  "homepage": "https://github.com/jupyterlab/jupyterlab-statusbar",
  "bugs": {
    "url": "https://github.com/jupyterlab/jupyterlab-statusbar/issues"
  },
  "license": "BSD-3-Clause",
  "author": "Richa Gadgil",
  "files": [
    "lib/**/*.d.ts",
    "lib/**/*.js.map",
    "lib/**/*.js",
    "style/*.css",
    "schema/*.json"
  ],
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "directories": {
    "lib": "lib/"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jupyterlab/jupyterlab-statusbar.git"
  },
  "scripts": {
    "build": "npm run lint:ts && tsc",
    "clean": "rimraf lib",
    "watch": "tsc -w  --listEmittedFiles",
    "lint:ts": "tslint -p tsconfig.json --exclude '**/*.d.ts'"
  },
  "dependencies": {
    "@jupyterlab/application": "^0.17.0-0",
    "@jupyterlab/apputils": "^0.17.0-0",
    "@jupyterlab/console": "^0.17.0-1",
    "@jupyterlab/coreutils": "^2.0.0-0",
    "@jupyterlab/fileeditor": "^0.17.0-1",
    "@jupyterlab/mainmenu": "^0.6.0-0",
    "@jupyterlab/notebook": "^0.17.0-0",
    "@jupyterlab/observables": "^2.0.0-1",
    "@jupyterlab/services": "^3.0.0-0",
    "@jupyterlab/terminal": "^0.17.0-1",
    "@phosphor/algorithm": "^1.1.2",
    "@phosphor/application": "^1.6.0",
    "@phosphor/commands": "^1.5.0",
    "@phosphor/coreutils": "^1.3.0",
    "@phosphor/disposable": "^1.1.2",
    "@phosphor/signaling": "^1.1.2",
    "@phosphor/virtualdom": "^1.1.2",
    "@phosphor/widgets": "^1.6.0",
    "react": "~16.4.0",
    "react-dom": "~16.4.0"
  },
  "devDependencies": {
    "@types/react": "~16.0.19",
    "@types/react-dom": "~16.0.2",
    "rimraf": "^2.6.1",
    "tslint": "^5.10.0",
    "typescript": "^2.9.2"
  },
  "jupyterlab": {
    "extension": true,
    "schemaDir": "schema"
  }
}
